<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cloud.dao.Song_infodao">
    <insert id="add_count" parameterType="cloud.entities.Song_info" >
        insert into song_info(songid,songname,score,playtime,singer)  values(#{songid},#{songname},#{score},#{playtime},#{singer});
    </insert>

</mapper>